@inject NavigationManager nm
<div class="modal fade show" id="myModal" style="display:block; background-color: rgba(10,10,10,.8);" aria-modal="true" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Disclamer</h4>
                <button type="button" class="close" @onclick="@ModelCancel">&times;</button>
            </div>
            <div class="modal-body">
                <p>For View Details Login first</p>
            </div>
            <div class="modal-footer">
               <button type="submit" class="btn btn-primary" @onclick="@ModalOk">Cancel</button>
                <button type="submit" class="btn btn-danger" @onclick="FunDel">Login</button>
            </div>
            
        </div>
    </div>
</div>

@code{
   
   
   

    public void FunDel()
    {
        nm.NavigateTo("/login");
    }
    [Parameter]
    public EventCallback<bool>OnClose { get; set; }
    private Task ModelCancel()
    {
        return OnClose.InvokeAsync(false);
    }
    private Task ModalOk()
    {
        return OnClose.InvokeAsync(true);
    }
    
    
    

}